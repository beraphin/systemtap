global res
probe begin {
	printf("BEGIN\n")
        x = 1; y = 0; z = -1
	res = x > 0 && y == 0 && z < 0 && 1 > 0 && 0 == 0 && -1 < 0
	      && 0 < x && 0 == y && 0 > z && 0 < 1 && 0 > -1
	      && x >= 0 && y == 0 && z <= 0 && 1 >= 0 && 0 == 0 && -1 <= 0
              && 0 <= x && 0 == y && 0 >= z && 0 <= 1 && 0 >= -1

}

probe kernel.function("vfs_read") {
	x = 1; y = 0; z = -1
	res = res && x > 0 && y == 0 && z < 0 && 1 > 0 && 0 == 0 && -1 < 0
	      && 0 < x && 0 == y && 0 > z && 0 < 1 && 0 > -1
              && x >= 0 && y == 0 && z <= 0 && 1 >= 0 && 0 == 0 && -1 <= 0
              && 0 <= x && 0 == y && 0 >= z && 0 <= 1 && 0 >= -1
	exit()
}

probe end {
	x = 1; y = 0; z = -1
	if (res && x > 0 && y == 0 && z < 0 && 1 > 0 && 0 == 0 && -1 < 0
            && 0 < x && 0 == y && 0 > z && 0 < 1 && 0 > -1
            && x >= 0 && y == 0 && z <= 0 && 1 >= 0 && 0 == 0 && -1 <= 0
            && 0 <= x && 0 == y && 0 >= z && 0 <= 1 && 0 >= -1)
		printf("END PASS\n")
	else
		printf("END FAIL\n")
}
