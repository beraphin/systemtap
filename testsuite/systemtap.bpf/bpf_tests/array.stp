global arr[3], flag = 1
probe begin {
	printf("BEGIN\n")
	arr[0] = 1
	arr[10] = 2
	arr[199] = 3
}

probe kernel.function("vfs_read") {
	if (flag) {
	    arr[0]++
            arr[10]++
	    arr[199]++
	    flag = 0
	    exit()
	}
}

probe end {
	if (arr[0] == 2 && arr[10] == 3 && arr[199] == 4)
		printf("END PASS\n")
	else
		printf("END FAIL\n")
}
