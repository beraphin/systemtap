#! stap -p4


probe nfs.proc.lookup {
	printf("%s(%s)\n", name, argstr)
	printf("%d %d %d %d\n", server_ip, prot, version, name_len)
	printf("%d %d\n", bitmask0, bitmask1)
}
probe nfs.proc.lookup.return {
	printf("%d: %s(%s)\n", version, name, retstr)
}

