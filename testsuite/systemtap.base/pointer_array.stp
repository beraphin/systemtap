probe kernel.function("do_execve")
{
    if (pid() == target()) {
        println(user_string(@defined($__argv) ? $__argv[0] : $argv[0]))
        for (i=0; i<9; ++i)
            printf("%c\n", @defined($__argv) ? $__argv[0][i] : $argv[0][i])
        println(@defined($__argv) ? $__argv[0][9] : $argv[0][9])
    }
}
