set test "poll_map"
if {![installtest_p]} { untested $test; return }

spawn stap -g $srcdir/$subdir/poll_map.stp
set ok 0
expect {
    -timeout 400
    -ex "SUCCESS" { incr ok }
    timeout { fail "$test (timeout)" }
    eof { }
}
catch { close }
wait
if {$ok == 1} { pass "$test ($ok)" } { fail "$test ($ok)" }
